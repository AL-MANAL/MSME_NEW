        function AnnexureApprovalNoty(idAnnexure, DocName, DocRef, PendingFlg, Document) {

            var element = document.getElementById("Annexure " + idAnnexure);
            $(element).block({
                message: '<i class="icon-spinner9 spinner"></i>',
                overlayCSS: {
                    backgroundColor: '#1B2024',
                    opacity: 0.85,
                    cursor: 'wait'
                },
                css: {
                    border: 0,
                    padding: 0,
                    backgroundColor: 'none',
                    color: '#fff'
                }
            });

            jQuery.ajax({
                url: '@Url.Action("AnnexureApprovalNoty", "MgmtDocuments")',
                        type: 'POST',
                        dataType: "json",
                        data: { 'idAnnexure': idAnnexure, 'DocName': DocName, 'DocRef': DocRef, 'PendingFlg': PendingFlg, 'Document': Document },
                        success: function (result) {

                            if (result == "Success") {
                                new Noty({
                                    text: "Document Approved successfully",
                                    type: 'success'
                                }).show();

                            }
                            else {
                                new Noty({
                                    text: 'Document Approved Failed',
                                    type: 'error'
                                }).show();

                            }

                            element.parentNode.removeChild(element);
                            bellcountdecrease();
                        }
                    });
                }


