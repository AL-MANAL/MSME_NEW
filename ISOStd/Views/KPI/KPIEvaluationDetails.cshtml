                            <p class="bg-brown py-1 px-2 rounded">
                                @Html.DisplayNameFor(model => model.kpi_level)
                            </p>
                        </td>
                        <td class="col">
                            <p class="bg-dark py-1 px-2 rounded">
                                @Html.DisplayFor(model => model.kpi_level)
