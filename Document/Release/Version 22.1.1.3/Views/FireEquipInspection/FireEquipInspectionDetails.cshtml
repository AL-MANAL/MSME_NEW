                            <p class="bg-brown py-1 px-2 rounded ">

                                @Html.DisplayNameFor(model => model.Fire_Water_Pumps_Remarks)
                            </p>
                        </td>
                        <td class="col">
                            <p class="bg-dark py-1 px-2 rounded">

                                @Html.DisplayFor(model => model.Fire_Water_Pumps_Remarks)
                            </p>

                        </td>
                        <td class="col-xs-3 col-md-2">

                            <p class="bg-brown py-1 px-2 rounded ">

                                @Html.DisplayNameFor(model => model.Fire_Box_No)
                            </p>
                        </td>
                        <td class="col">
                            <p class="bg-dark py-1 px-2 rounded">

                                @Html.DisplayFor(model => model.Fire_Box_No)
                            </p>
                        </td>
                    </tr>
                    <tr class="row">
                        <td class="col-xs-3 col-md-2">

                            <p class="bg-brown py-1 px-2 rounded ">

                                @Html.DisplayNameFor(model => model.Fire_Box_No_Location)
