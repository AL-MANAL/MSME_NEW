                                {
                                    @Html.DisplayNameFor(model => model.approved_by_approve)
                                }
                                else
                                {
                                    @Html.DisplayNameFor(model => model.approved_by)
                                }
                               
                            </p>
                        </td>
                        <td class="col">
                            <p class="bg-dark py-1 px-2 rounded">
                                @Html.DisplayFor(model => model.approved_by)
