            @if (dsSource != null && dsSource.Tables.Count > 0 && dsSource.Tables[0].Rows.Count > 0)
            {
                <br />
                <h2 class="bg-dark pl-sm-2">Causes for Failure</h2>
                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-hover datatable-responsive" id="table1">
                        <thead>
                            <tr class="bg-brown">
                                <th>Sl No</th>
                                <th>Potential Causes for Failure</th>                   
                            </tr>
                        </thead>
                        <tbody>
                            @for (int i = 0, j = 1; i < dsSource.Tables[0].Rows.Count; i++, j++)
                            {
                                <tr>

                                    <td>
                                        @j
                                    </td>
                                    <td>
                                        @dsSource.Tables[0].Rows[i]["causes_failure"].ToString()
                                    </td>
                                   
                                </tr>
                            }
                        </tbody>
                    </table>



                </div>
            }
            @if (dsAction != null && dsAction.Tables.Count > 0 && dsAction.Tables[0].Rows.Count > 0)
            {
                <br />
                <h2 class="bg-dark pl-sm-2">Action Plan</h2>
                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-hover datatable-responsive" id="table1">
                        <thead>
                            <tr class="bg-brown">
                                <th>Sl No</th>
                                <th>Action details</th>
                                <th>Person Responsible</th>
                                <th>Target Date</th>
                                <th>Remarks</th>
                            </tr>
                        </thead>
                        <tbody>
                            @for (int i = 0, j = 1; i < dsAction.Tables[0].Rows.Count; i++, j++)
                            {
                                <tr>

                                    <td>
                                        @j
                                    </td>
                                    <td>
                                        @dsAction.Tables[0].Rows[i]["Action"].ToString()
                                    </td>
                                    <td>
                                        @Obj.GetEmpHrNameById(dsAction.Tables[0].Rows[i]["PersonResponsible"].ToString())
                                    </td>
                                    <td>
                                        @if (dsAction.Tables[0].Rows[i]["TargetDate"].ToString() != null && Convert.ToDateTime(dsAction.Tables[0].Rows[i]["TargetDate"].ToString()) > Convert.ToDateTime("01/01/0001"))
                                        {
                                            @Html.Label(Convert.ToDateTime(dsAction.Tables[0].Rows[i]["TargetDate"].ToString()).ToString("dd/MM/yyyy"))
                                        }
                                    </td>
                                  
                                    <td>
                                        @dsAction.Tables[0].Rows[i]["remarks"].ToString()
                                    </td>

                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            }
            @if (dsMI != null && dsMI.Tables.Count > 0 && dsMI.Tables[0].Rows.Count > 0)
            {
                <br />
                <h2 class="bg-dark pl-sm-2">KPI Evaluation</h2>
                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-hover datatable-responsive" id="table1">
                        <thead>
                            <tr class="bg-brown">
                                <th>Sl No</th>
                                <th>MI_No</th>
                                <th>Measurable Indicator</th>
                                <th>Expected value to be achieved</th>
                                <th>KPI Type</th>
                                <th>Monitoring Period From Date</th>
                                <th>To Date</th>
                                <th>Monitoring Mechanism</th>
                                <th>Frequency of Evaluation</th>
                                <th>Risk, if not achieved</th>
                                <th>Evaluation Date</th>
                                <th>Source_of_data to evaluate</th>
                                <th>Upload</th>
                                <th>Method of evaluation</th>
                                <th>Value achieved</th>
                                <th>KPI Status</th>
                                <th>Remarks</th>
                                <th>NCR Raise Need</th>
                                <th>Raise NCR</th>
                            </tr>
                        </thead>
                        <tbody>
                            @for (int i = 0, j = 1; i < dsMI.Tables[0].Rows.Count; i++, j++)
                            {
                                <tr>

                                    <td>
                                        @j
                                    </td>
                                    <td>
                                        @dsMI.Tables[0].Rows[i]["kpi_ref_no"].ToString()
                                    </td>
                                    <td>
                                        @dsMI.Tables[0].Rows[i]["measurable_indicator"].ToString()
                                    </td>
                                    <td>
                                        @dsMI.Tables[0].Rows[i]["expected_value"].ToString()/@dsMI.Tables[0].Rows[i]["expected_value_unit"].ToString()
                                    </td>
                                    <td>
                                        @Obj.GetKPITypeById(dsMI.Tables[0].Rows[i]["kpi_type"].ToString())
                                    </td>
                                    <td>
                                        @if (dsMI.Tables[0].Rows[i]["monitoring_frm_date"].ToString() != null && Convert.ToDateTime(dsMI.Tables[0].Rows[i]["monitoring_frm_date"].ToString()) > Convert.ToDateTime("01/01/0001"))
                                        {
                                            @Html.Label(Convert.ToDateTime(dsMI.Tables[0].Rows[i]["monitoring_frm_date"].ToString()).ToString("dd/MM/yyyy"))
                                        }
                                    </td>
                                    <td>
                                        @if (dsMI.Tables[0].Rows[i]["monitoring_to_date"].ToString() != null && Convert.ToDateTime(dsMI.Tables[0].Rows[i]["monitoring_to_date"].ToString()) > Convert.ToDateTime("01/01/0001"))
                                        {
                                            @Html.Label(Convert.ToDateTime(dsMI.Tables[0].Rows[i]["monitoring_to_date"].ToString()).ToString("dd/MM/yyyy"))
                                        }
                                    </td>
                                    <td>
                                        @*Obj.GetKPIMonitoringMechanismById*@ @(dsMI.Tables[0].Rows[i]["monitoring_mechanism"].ToString())
                                    </td>
                                    <td>
                                        @Obj.GetKPIFrequencyEvaluationById(dsMI.Tables[0].Rows[i]["frequency_eval"].ToString())
                                    </td>
                                    <td>
                                        @dsMI.Tables[0].Rows[i]["risk"].ToString()
                                    </td>
                                    <td>
                                        @if (dsMI.Tables[0].Rows[i]["evaluation_date"].ToString() != null && dsMI.Tables[0].Rows[i]["evaluation_date"].ToString() != "" && Convert.ToDateTime(dsMI.Tables[0].Rows[i]["evaluation_date"].ToString()) > Convert.ToDateTime("01/01/0001"))
                                        {
                                            @Html.Label(Convert.ToDateTime(dsMI.Tables[0].Rows[i]["evaluation_date"].ToString()).ToString("dd/MM/yyyy"))
                                        }
                                    </td>
                                    <td>
                                        @dsMI.Tables[0].Rows[i]["source_evaluate"].ToString()
                                    </td>
                                    <td>
                                        @if (dsMI.Tables[0].Rows[i]["upload"].ToString() != null && dsMI.Tables[0].Rows[i]["upload"].ToString() != "" && dsMI.Tables[0].Rows[i]["upload"].ToString().Length > 0)
                                        {
                                            foreach (var docs in dsMI.Tables[0].Rows[i]["upload"].ToString().Trim(',').Split(','))
                                            {
                                                <p style="margin: 0px;">

                                                    <a href='@Url.Action("DisplayDocument", "Reports", new { Document = docs })' target="_blank" style="color: orange;">
                                                        Preview
                                                    </a>
                                                </p>
                                                <br />
                                            }
                                        }
                                    </td>
                                    <td>
                                        @dsMI.Tables[0].Rows[i]["method_evaluation"].ToString()
                                    </td>
                                    <td>
                                        @dsMI.Tables[0].Rows[i]["value_achieved"].ToString()
                                    </td>
                                    <td>
                                        @Obj.GetKPIEvaluationStatusById(dsMI.Tables[0].Rows[i]["kpi_status"].ToString())
                                    </td>
                                    <td>
                                        @dsMI.Tables[0].Rows[i]["remarks"].ToString()
                                    </td>
                                    <td>
                                        @dsMI.Tables[0].Rows[i]["raise_need"].ToString()
                                    </td>
                                    <td>
                                        <a href='@Url.Action("AddNC", "NC")' title="Raise NCR " target="_blank">
                                            <span class="badge badge-info">Raise NCR</span>
                                        </a>
                                    </td>

                                </tr>
                            }
                        </tbody>
                    </table>



                </div>
            }
        </div>




    </div>
</div>

