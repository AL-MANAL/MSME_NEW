                    </div>
                </td>
                <td>
                    <div class="editor-field">
                        @Html.DisplayFor(model => model.jobno)
                    </div>
                </td>
            </tr>
          <tr>
 <td>
                    <div class="editor-label">
                        @Html.LabelFor(model => model.po)
