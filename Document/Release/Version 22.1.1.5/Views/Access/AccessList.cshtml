        function Send() {
            var selected = "";
            var selected1 = "";
            $('#checkboxes input:checked').each(function () {
                // selected.push($(this).attr('name'));
                selected = selected + $(this).attr('name') + ",";
                selected = selected + $(this).attr('value') + ",";
            });
            //SendEmail(selected);
